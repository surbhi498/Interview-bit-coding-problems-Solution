#include <iostream>
#include <iterator>
#include <map>
  
using namespace std;
class Solution {
public:
    int singleNonDuplicate(vector<int>& nums) {
       map<int,int> mp;
       int n=nums.size(); 
       for(int i=0;i<n;i++){
           mp[nums[i]]++;
       } 
        int x;
        for(int i=0;i<n;i++){
         if(mp[nums[i]]==1)
            x=nums[i];
        }
    return x;
    
    }
    
    
};
